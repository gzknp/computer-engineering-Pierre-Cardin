<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="cengPC.ProfilPage" BackgroundColor="White">
    <NavigationPage.TitleView>
        <Label Margin="0,15,65,50" HorizontalTextAlignment="Center" 
               TextColor="Black" 
               Text="Profilim" 
               FontAttributes="Bold"
              FontSize="18" ></Label>
    </NavigationPage.TitleView>
  <ContentPage.Content>
        <StackLayout>
            <ListView x:Name="UyeListView" >
                <ListView.ItemTemplate>
                    <DataTemplate>
                        <TextCell Text="{Binding Ad}"/>
                    </DataTemplate>
                </ListView.ItemTemplate>
            </ListView>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>